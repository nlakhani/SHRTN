{
  "name": "SHRTN",
  "description": "Clean up tabs and get a summary every day",
  "version": "0.2",
  "incognito": "split",
  "browser_action": {
   "default_icon": "shrtn.png",
   "default_popup": "shrtn.html"
  },
  "permissions": [
   "activeTab",
   "storage",
   "file://*/*",
   "http://*/*"
   ],
"manifest_version": 2,
"permissions": ["tabs", "storage", "file://*/*", "http://*/*"]
}
